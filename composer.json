{
    "name": "evanshunt/elemental-addons",
    "description": "A collection of CMS blocks for the silverstripe-elemental module for use in EH projects",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/evanshunt/eh-ss-blocks",
    "support": {
        "issues": "https://github.com/evanshunt/eh-ss-blocks/issues"
    },
    "require": {
        "silverstripe/vendor-plugin": "^1|^2",
        "silverstripe/cms": "^4|^5",
        "silverstripe/framework": "^4|^5",
        "dnadesign/silverstripe-elemental": "^4|^5",
        "dnadesign/silverstripe-elemental-list": "^1.2|^2",
        "bummzack/sortablefile": "^2.0"

    },
    "extra": {
        "installer-name": "elemental-addons",
        "expose": [
            "js"
        ]
    },
    "autoload": {
        "psr-4": {
            "EvansHunt\\Elements\\": "src/"
        }
    }
}
